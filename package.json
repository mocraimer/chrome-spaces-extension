{
  "name": "chrome-spaces",
  "version": "1.0.0",
  "description": "A Chrome extension for managing windows as workspaces",
  "private": true,
  "scripts": {
    "clean": "rm -rf build/ chrome-spaces.zip",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --mode production --config webpack.config.ts",
    "build:zip": "cd build && zip -r ../chrome-spaces.zip .",
    "dev": "npm run clean && cross-env NODE_ENV=development webpack --mode development --config webpack.config.ts",
    "watch": "webpack --watch --mode development",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write '**/*.{ts,tsx,css,html,json}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:stability": "node e2e-tests/run-stability-tests.js",
    "test:stability:comprehensive": "playwright test e2e-tests/comprehensive-stability.test.ts",
    "test:stability:persistence": "playwright test e2e-tests/space-name-persistence.test.ts",
    "test:stability:restoration": "playwright test e2e-tests/spaceRestoration.test.ts",
    "test:stability:session": "playwright test e2e-tests/session-restore.test.ts",
    "test:stability:performance": "playwright test e2e-tests/performance-benchmarks.test.ts",
    "test:stability:recovery": "playwright test e2e-tests/error-recovery-edge-cases.test.ts",
    "test:bdd": "cucumber-js",
    "test:bdd:tag": "cucumber-js --tags",
    "test:bdd:smoke": "cucumber-js --tags @smoke",
    "test:bdd:core": "cucumber-js --tags @core",
    "test:bdd:report": "cucumber-js --format html:cucumber-report.html",
    "test:bdd:dryrun": "cucumber-js --dry-run",
    "test:visual": "node scripts/run-visual-tests.js",
    "test:visual:update": "node scripts/run-visual-tests.js --update-baselines",
    "test:visual:debug": "node scripts/run-visual-tests.js --debug",
    "test:visual:headed": "node scripts/run-visual-tests.js --headed",
    "test:visual:spaces": "node scripts/run-visual-tests.js --test space-states",
    "test:visual:dialogs": "node scripts/run-visual-tests.js --test dialogs-states",
    "test:visual:ui": "node scripts/run-visual-tests.js --test ui-stability"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/chrome-spaces.git"
  },
  "keywords": [
    "chrome-extension",
    "tabs",
    "workspace",
    "productivity"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cucumber/cucumber": "^11.3.0",
    "@cucumber/pretty-formatter": "^1.0.1",
    "@playwright/test": "^1.51.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/chrome": "^0.0.254",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-virtualized-auto-sizer": "^1.0.8",
    "@types/react-window": "^1.8.8",
    "@types/redux-mock-store": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "ajv": "^8.17.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.0.0",
    "eslint": "^8.0.0",
    "html-webpack-plugin": "^5.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.0",
    "redux-mock-store": "^1.5.5",
    "style-loader": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.0",
    "@types/styled-components": "^5.1.34",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^8.1.3",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "styled-components": "^6.1.17"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/jest.setup.ts"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "<rootDir>/src/tests/mocks/styleMock.ts",
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json"
        }
      ]
    }
  }
}
